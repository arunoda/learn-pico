ES8388 R0=0x46 (0100 0110) // set to defaults: 0000 0110 
ES8388 R1=0x50 (0101 0000) // need to enable analog power: 0101 0000
ES8388 R2=0x00 (0000 0000) // enable chip power: 0000 0000
ES8388 R3=0xFC (1111 1100) // enable ADCs: 0000 1100
ES8388 R4=0x30 (0011 0000) // enable DACs: 0011 1100
ES8388 R5=0x00 (0000 0000) // no low power mode: 0000 0000
ES8388 R6=0x00 (0000 0000) // no low power mode: 0000 0000
ES8388 R7=0x7C (0111 1100) // anaog voltage mgt: 0111 1100
ES8388 R8=0x80 (1000 0000) // master/slave mode: master mode selected: 1000 0000
ES8388 R9=0x00 (0000 0000) // mic gain to 0db: 0000 0000
ES8388 R10=0x00 (0000 0000)
ES8388 R11=0x02 (0000 0010)
ES8388 R12=0x00 (0000 0000)
ES8388 R13=0x06 (0000 0110)
ES8388 R14=0x30 (0011 0000)
ES8388 R15=0x20 (0010 0000)
ES8388 R16=0xC0 (1100 0000) // ADCL volume, we need to set it to 0000 0000 of 0db
ES8388 R17=0xC0 (1100 0000) // ADCR volume, we need to set it to 0000 0000 of 0db
ES8388 R18=0x38 (0011 1000)
ES8388 R19=0xB0 (1011 0000)
ES8388 R20=0x32 (0011 0010)
ES8388 R21=0x06 (0000 0110)
ES8388 R22=0x00 (0000 0000)
ES8388 R23=0x18 (0001 1000) // DAC 16bit, i2s is selected
ES8388 R24=0x02 (0000 0010) // Dac to MCLK is 256 selected
ES8388 R25=0x22 (0010 0010)
ES8388 R26=0xC0 (1100 0000) // We need to set to 0000 0000 for 0db for DACL
ES8388 R27=0xC0 (1100 0000) // We need to set to 0000 0000 for 0db for DACR
ES8388 R28=0x00 (0000 0000) // Set to default: 0000 1000 
ES8388 R29=0x00 (0000 0000)
ES8388 R30=0x1F (0001 1111)
ES8388 R31=0xF7 (1111 0111) 
ES8388 R32=0xFD (1111 1101)
ES8388 R33=0xFF (1111 1111)
ES8388 R34=0x1F (0001 1111)
ES8388 R35=0xF7 (1111 0111)
ES8388 R36=0xFD (1111 1101)
ES8388 R37=0xFF (1111 1111)
ES8388 R38=0x00 (0000 0000) // LIN select: 0000 0000 (LIN1 -> LEFT, RIN1 -> RIGHT)
ES8388 R39=0x38 (0011 1000) // need to enable LIN to Mixer and 0db volume 0101 0000
ES8388 R40=0x28 (0010 1000)
ES8388 R41=0x28 (0010 1000)
ES8388 R42=0x38 (0011 1000) // need to enable RIN to Mixer and 0db volume 0101 0000
ES8388 R43=0x00 (0000 0000)
ES8388 R44=0x00 (0000 0000)
ES8388 R45=0x00 (0000 0000)
ES8388 R46=0x00 (0000 0000) // LOUT1 volume: need to change to 0111 1000 for 0db
ES8388 R47=0x00 (0000 0000) // ROUT1 volume: need to change to 0111 1000 for 0db
ES8388 R48=0x00 (0000 0000) // LOUT2 volume: need to change to 0111 1000 for 0db
ES8388 R49=0x00 (0000 0000) // ROUT2 volume: need to change to 0111 1000 for 0db
ES8388 R50=0x00 (0000 0000)
ES8388 R51=0xAA (1010 1010)
ES8388 R52=0xAA (1010 1010)